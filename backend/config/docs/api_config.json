{

    
    "app-name" : "MitoCube",
    "welcome-text" : "MitoCube offers protein-centric searches to explore the expression of a protein in all acquired proteomic datasets.",
    "base-url" : "https://mitocube.com/api/",
    "website-pw": "mitocube-pw",
    "email-sever-settings" : {
        "MAIL_SERVER": "mail.privateemail.com", 
        "MAIL_PORT": 465, 
        "MAIL_USERNAME": "support@mitocube.com", 
        "MAIL_USE_TLS": 0, 
        "MAIL_USE_SSL": 1,
        "MAIL_DEFAULT_SENDER" : "support@mitocube.com"
    },
    "email-cc-submission-list" : ["hendrik.nolte@age.mpg.de","Dominique.Diehl@age.mpg.de"], 
    "email-pw": "email-pw",
    "api":{
        "token-valid(h)" : 48,
        "share-token-valid(h)" : 3000,
        "pages" : {
            "protein" : 1,
            "dataset" : 1,
            "ptm" : 1,
            "submission" : 1,
            "performance" : 0,
            "admin" : 1  
        },
        "db-feature-annotations" : ["Entry","Protein names","Gene names","Gene names  (primary )","Organism"],
        "db-summary-params" : [
                "Gene names",
                "Subcellular location [CC]",
                "Function [CC]",
                "Domain [CC]",
                "Gene ontology (cellular component)",
                "Interacts with",
                "ComplexName",
                "MitoCarta3.0_List",
                "MitoCarta3.0_Evidence",
                "MitoCarta3.0_SubMitoLocalization",
                "MitoCarta3.0_MitoPathways",
                "Tissues",
                "MitoCoP",
                "MitoCoP disease gene",
                "MitoCoP disease association",
                "Functional MitoCoP classification"
                ],
        "filter-params" : ["Type","Organism","Material"],
        "annotation-colum" : "Gene names  (primary )",
        "feature-sort-by" : "Gene names  (primary )",
        "feature-item-labels" : {
            "main" : "Gene names  (primary )",
            "id" : "Entry",
            "sub-main" : "Gene names",
            "bold-sub-text" : "Organism",
            "info" : "Protein names"
        },
        "extra-colums-in-dataset-heatmaps" : ["MitoCarta3.0_List","MitoCoP"],
        "default-number-clusters-heatmap" : 8,
        "filters-in-volcano-plots" : ["MitoCarta3.0_List","MitoCoP"],
        "highlight-in-volcano-plots" : ["MitoCarta3.0_SubMitoLocalization","MitoCarta3.0_MitoPathways"],
        "highlight-in-volcano-plots-sep-cat-by" : {
            "MitoCarta3.0_MitoPathways" : ">"},

        "detail-params" : ["Creation Date","Title","Experimentator","Type","Organism","Material","groupingNames","Number Samples","Number Replicates","Number Groupings","Experimental Info"],
        "dataset-tag-names" : ["Creation Date","Type","Experimentator","Organism","Material","Number Samples","dataID"],
        "dataset-search-names" : ["Creation Date","Type","Experimentator","Organism","Material","dataID","Title"],
        "dataset-header" : "Title",
        "dataset-presentation" : ["Creation Date","Title","Material","Type","Organism","Experimentator","Number Samples"],
        "dataset-presentation-sort-by" : ["Organism","Type","Creation Date"],

        "dataset-admin-table-params" : ["Creation Date","dataID","Title","Experimentator","GroupName","Protein of Interest","Type","Organism","Material","Number Samples"],
        "dataset-admin-email-param" : "Email",

        "experiment-procedure-params" : ["Creation Date","dataID","Experimentator","Material","Title","Number Samples","Number Replicates","groupingNames","Experimental Info"],
        "short-cut-filter-param" : "Type",
        "short-cut-colors" : ["#365072","#6e5b7b","#b66476","#e8696e","#eaac89","#8c7f77"],
        "submission-allow-custom-runName" : 1, 
        "submission-states" : ["Submitted","Processed","Measuring","Measuring Paused","Data analysis","Done"], 
        "submission-search-columns" : ["Material","Organism","dataID","Title","Email","Type","Experimentator"],
        "submission-tags" : ["Experimentator","Email","Type","Organism","Material","dataID"],
        "submission-summary" : ["Creation Date", "State", "dataID","Email","Experimentator","Title","SampleNumber","Type","Organism", "Material","Experimental Info","groupingNames"],
        "submission-summary-short" : [
            "Creation Date",
            "State",
            "dataID",
            "Title",
            "Experimentator",
            "Type",
            "Material",
            "SampleNumber",
            "Organism",
            "Title",
            "groupingNames",
            "daysFromCreationToLastState"],
        "submission-summarize-as-experimentalInfo" : ["Research Aim","Sample Preparation","Additional Information"],
        "submission-details" : [
            {
                "field" : "text-input",
                "name" : "State",
                "q" : "State",
                "default" : "Submitted",
                "placeholder" : "State of your project.",
                "disabled" : "True"
            },
            {
                "field" : "date-input",
                "name" : "Creation Date",
                "q" : "Date of Sample Submission",
                "default" : "today",
                "placeholder" : "Enter date of sample  submission."
            },
            {
                "field" : "text-input",
                "name" : "Experimentator",
                "q" : "Name (First and last)",
                "default" : "",
                "placeholder" : "Enter your name."
            },
            {
                "field" : "text-input",
                "name" : "Email",
                "q" : "Email adress",
                "default" : "",
                "placeholder" : "Enter your email adress. You can enter multiple emails by separating them with a comma.",
                "intent" : "primary",
                "leftIcon":"envelope"
            },
            {
                "field" : "text-input",
                "name" : "GroupName",
                "q" : "Group Name",
                "default" : "Langer Department",
                "placeholder" : "Enter the group / principal investigator name."
            },
            {
                "field" : "text-input",
                "name" : "Title",
                "q" : "Project Title",
                "default" : "",
                "placeholder" : "Enter your project's title."
            },
            {
                "field" : "text-input",
                "name" : "Research Question",
                "q" : "Research Question",
                "default" : "",
                "placeholder" : "What question would you like to answer with your project."
            },
            {
                "field" : "textfield-input",
                "name" : "Research Aim",
                "title" : "Research Aim",
                "q" : "Research Aim",
                "default" : "",
                "placeholder" : "Please provide a short description of your project and it's research aims."
            },
            {
                "field" : "text-input",
                "name" : "Protein of Interest",
                "title" : "Protein of Interest",
                "q" : "Protein of Interest (seperate multiple by comma)",
                "default" : "",
                "placeholder" : "Please provide the protein(s) of interest (knockout line, OMA1, YME1l1).",
                "intent" : "primary"
            },
            {
                "field" : "text-input",
                "name" : "Linked data",
                "title" : "Linked data",
                "q" : "Linked data to connect whole proteome and PTM data.",
                "default" : "None",
                "placeholder" : "Please provide a dataID if a linked submission/dataset exists. Example: Phospho- and whole proteome datasets."
            },
            {
                "field" : "combo-input",
                "name" : "Organism",
                "q" : "Organism",
                "text" : "Organism",
                "items" : [
                    "Homo sapiens (Human)", 
                    "Mus musculus (Mouse)", 
                    "Saccharomyces cerevisiae (Baker's Yeast)",
                    "Caenorhabditis elegans", 
                    "Other/Multiple (Specify in Additional Info)"]
            },
            {
                "field" : "combo-input",
                "name" : "Type",
                "q" : "Experiment type",
                "text" : "Experiment Type",
                "items" : [
                    "Whole proteome",
                    "Neo N-term enrichment",
                    "Phophoproteome",
                    "Acetylome",
                    "Ubiquitinome",
                    "Pulse-SILAC",
                    "Immunoprecipitation",
                    "Other (Specify in Additional Info)"]
            },
            {
                "field" : "text-input",
                "name" : "Material",
                "q" : "Material",
                "default" : "",
                "placeholder" : "Material used (HeLa,Liver,..)"
            },
            {
                "field" : "textfield-input",
                "name" : "Sample Preparation",
                "title" : "Sample preparation",
                "q" : "Sample preparation",
                "default" : "",
                "placeholder" : "Please provide details about the experimental procedure of cell culture / treatments. In addition, provide the cell culture media and supplements. Please enter SILAC labelling information here as well."
            },
            {
                "field" : "textfield-input",
                "name" : "Additional Information",
                "title" : "Additional Information",
                "q" : "Notes",
                "default" : "",
                "placeholder" : "Please provide additional information if required. Examples might be about a bias in sample preparation. Experimental errors.  Batch effects."
            },
            {
                "field" : "numeric-input",
                "name" : "Number Samples",
                "q" : "Number of samples",
                "default" : "",
                "placeholder" : "Number of samples.",
                "min" : 1,
                "max" : 9999,
                "defaultValue" : 2,
                "minorStepSize": 1
            },
            {
                "field" : "numeric-input",
                "name" : "Number Replicates",
                "q" : "Number of replicates",
                "default" : "",
                "placeholder" : "Number of replicates per group.",
                "min" : 1,
                "max" : 9999,
                "defaultValue" : 2,
                "minorStepSize": 1
            },
            {
                "field" : "numeric-input",
                "name" : "Number Groupings",
                "q" : "Number of groupings",
                "default" : "",
                "placeholder" : "Number of groupings (Genotype, Treatment, ..).",
                "min" : 1,
                "max" : 9999,
                "defaultValue" : 2,
                "minorStepSize": 1
            }
        ],

        "performance-runs" : {
            "General" : ["Date","Researcher","Raw file"],
            "Properties" : ["Mass Spectrometer","Liquid Chromatography","Acquisition Mode","Column Length","FAIMS CV"],
            "Metrices" : ["Precursors.Identified", "Proteins.Identified","FWHM.RT","Median.Mass.Acc.MS2","MS2.Mass.Instability"],
            "Distributions" : ["CScore","Peak length","Median.Fragmentation.Correlation"],
            "QC-Peptides" : {
                "name" : ["LGGNEQVTR","YILAGVENSK","GTFIIDPGGVIR","GTFIIDPAAVIR","GAGSSEPVTGLDAK","TPVISGGPYEYR","VEATFGVDESNAK","TPVITGAPYEYR","DGLDAASYYAPVR","ADVTPADFSEWSK","LFLQFGAQGSPFLK"],
                "metrices" : ["Retention time","Peak length"]
            },
            "Miscs" : ["Comment"] 
            
        },
        "performancePropertyOptions" : {
            "Mass Spectrometer" : ["Exploris 480 #1","E1","E2","T1"],
            "Liquid Chromatography" : ["nanoLC 1200","Neo","Evo"],
            "Column Length" : ["40cm","30cm","25cm","15cm","8cm","50cm"],
            "Column Type" : ["In-House","Performance, Aurora"],
            "Material" : ["HeLa digest (Pierce)","In-house"],
            "Acquisition Mode" : ["DDA","DIA"],
            "Gradient Length" : ["60min","120min"],
            "FAIMS CV" : ["None","-50 V", "-45 V"]
        },
        "performance-main-group" : "Mass Spectrometer",
        "performance-metrices-colors" : {
            "Precursors.Identified" : "#5d7490",
            "Proteins.Identified" : "#73908d",
            "FWHM.RT" : "#d59f78",
            "Median.Mass.Acc.MS2" : "blue",
            "MS2.Mass.Instability" : "green"
        }
    } 
}

